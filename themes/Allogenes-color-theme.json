	"tokenColors": [
		{
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#3b4255"
			}
		},
		{
			"scope": [
				"entity.name",
			],
			"settings": {
				"foreground": "#3b4255",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
